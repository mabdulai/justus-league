{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "strictNullChecks": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "ignoreDeprecations": "5.0",
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "@styles/*": ["src/styles/*"]
        }
    }
}
